{
  "name": "azirax/documentation",
  "type": "application",
  "description": "Azirax documentation is a PHP API documentation generator. Fork of Sami",
  "keywords": [
    "phpdoc",
    "generator"
  ],
  "homepage": "https://github.com/phptux/azirax-documentation",
  "readme": "https://github.com/phptux/azirax-documentation#readme",
  "license": "MIT",
  "support": {
    "email": "php.tux@web.de",
    "issues": "https://github.com/phptux/azirax-documentation/issues",
    "source": "https://github.com/phptux/azirax-documentation"
  },
  "authors": [
    {
      "name": "Fabien Potencier",
      "email": "fabien@symfony.com"
    },
    {
      "name": "Rene Dziuba",
      "email": "php.tux@web.de"
    }
  ],
  "require": {
    "php": ">=8.3",
    "twig/twig": "v3.20.0",
    "nikic/php-parser": "v5.4.0",
    "symfony/console": "v7.2.5",
    "symfony/finder": "v7.2.0",
    "symfony/filesystem": "v7.2.0",
    "symfony/yaml": "v7.2.5",
    "symfony/process": "v7.2.5",
    "phpdocumentor/reflection-docblock": "5.6.1",
    "phpdocumentor/type-resolver": "1.10.0",
    "wdes/php-i18n-l10n": "v4.1.0",
    "code-lts/cli-tools": "v1.6.0",
    "erusev/parsedown": "1.7.4"
  },
  "autoload": {
    "psr-4": {
      "Azirax\\Documentation\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.20",
    "codeception/codeception": "5.2.1",
    "codeception/module-asserts": "3.0.0",
    "codeception/module-cli": "2.0.1",
    "codeception/module-filesystem": "3.0.1",
    "vlucas/phpdotenv": "v5.6.0",
    "friendsofphp/php-cs-fixer": "v3.75.0",
    "ext-pdo": "*"
  },
  "autoload-dev": {
    "psr-4": {
      "Azirax\\Documentation\\Tests\\Unit\\": "tests/Unit/",
      "Azirax\\Documentation\\Tests\\Fixtures\\": "tests/_data/fixtures/",
      "Azirax\\Documentation\\Tests\\Source\\": "tests/_data/source/"
    }
  },
  "prefer-stable": true,
  "bin": [
    "bin/azdoc"
  ]
}